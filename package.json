{
  "name": "czy-jest-tarta",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=DEV webpack-dev-server",
    "build": "NODE_ENV=production webpack --config webpack.prod.config.js",
    "server": "node server/index.js",
    "cover:mocha": "NODE_ENV=test babel-node node_modules/.bin/babel-istanbul cover _mocha --  --require test/helpers/browser.js test/**/*.spec.js --reporter dot || true",
    "test:mocha": "NODE_ENV=test mocha test/**/*.spec.js --compilers js:babel-core/register",
    "deploy": "npm run build && git add -A && git commit -m 'push' && git push && pm2 deploy ecosystem.json production",
    "cover": "nyc --reporter=html --reporter=text ava",
    "test": "ava",
    "test:watch": "ava --watch"
    },
  "ava": {
    "files": [
      "src/**/*.spec.js",
      "!test/*"
    ],
    "modules": false,
    "babel": {
      "plugins": [
        "rewire"
      ],
      "babelrc": true,
      "presets": [
        "@ava/stage-4",
        "stage-3"
      ]
    },
    "require": [
      "babel-polyfill",
      "babel-register",
      "./test/setupAva.js"
    ]
  },
  "dependencies": {
    "@slack/client": "^3.6.0",
    "autoprefixer": "^6.5.0",
    "ava": "^0.19.0",
    "babel-core": "^6.24.1",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-rewire": "^1.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-airbnb": "^2.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "browser-env": "^2.0.29",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.13.0",
    "deep-freeze-node": "^1.1.2",
    "deep-freeze-strict": "^1.1.1",
    "enzyme": "^2.8.0",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-react": "^6.10.3",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch": "^1.1.0",
    "firebase": "^3.0.5",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^2.28.0",
    "http": "0.0.0",
    "immutability-helper": "^2.0.0",
    "immutable": "^3.8.1",
    "jsdom": "latest",
    "json-loader": "^0.5.4",
    "marked": "^0.3.5",
    "material-ui": "^0.16.0",
    "mobile-detect": "^1.3.3",
    "node-fetch": "^1.6.3",
    "nyc": "^10.2.0",
    "path": "^0.12.7",
    "prop-types": "^15.5.4",
    "radium": "^0.18.1",
    "react": "latest",
    "react-addons-css-transition-group": "^15.3.2",
    "react-addons-test-utils": "^15.5.1",
    "react-bootstrap": "^0.30.5",
    "react-data-provider": "^1.0.1",
    "react-dom": "latest",
    "react-redux": "latest",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "latest",
    "redux-thunk": "latest",
    "rumble-charts": "^1.0.7",
    "socket.io": "^1.5.1",
    "socket.io-client": "^1.5.1",
